body {
    margin: 0px;
    padding: 0px;
    font-family: Arial, Helvetica, sans-serif;
    font-size: 12px;
    background-color: #FFFFFF!important;
}

@font-face {
    font-family: 'DBOzoneX';
    src: url(<%= asset_path 'db_Ozone.eot' %>);
    src: url(<%= asset_path 'db_Ozone.eot?#iefix' %>) format('embedded-opentype'), url(<%= asset_path 'db_Ozone.ttf' %>) format('truetype');
    font-weight: normal;
    font-style: normal;

}

p {
    margin: 0px;
    padding: 0px;
}

#mainpage {
    width: 100%;
}

#toppage-mainpage {
