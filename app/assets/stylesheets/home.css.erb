ul.small-menu {
    font-family: DBOzoneX;
    text-decoration: none;
    font-size: 18px;
    color: #FFF;
    width: 270px;
    margin: 9px 0 10px 0;
    padding: 0px;
    list-style: none;
    float: right;
}

