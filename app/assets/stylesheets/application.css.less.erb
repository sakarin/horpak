    @font-face {
        font-family: 'DBOzoneX';
        src: url(<%= asset_path 'db_Ozone.eot' %>);
        src: url(<%= asset_path 'db_Ozone.eot?#iefix' %>) format('embedded-opentype'), url(<%= asset_path 'db_Ozone.ttf' %>) format('truetype');
        font-weight: normal;
        font-style: normal;

    }

    .content {
        margin-top: 60px!important;
    }

    .navbar {
       padding-bottom: 20px;
    }

    .content {


        /*padding: 20px 20px 0px 20px;*/
        /*padding: 20px;*/

        min-height: 480px;
        background: #fff;
        -webkit-box-shadow: 0 0 6px rgba(0, 0, 0, 0.3);
        -moz-box-shadow: 0 0 6px rgba(0, 0, 0, 0.3);
        box-shadow: 0 0 6px rgba(0, 0, 0, 0.3);
        -webkit-border-radius: 3px;
        -moz-border-radius: 3px;
        border-radius: 3px;
    }

    body {
        /*background: url(<%= asset_path 'wallpapers/paper.jpg'%>) #f5efe6;*/
        /*-webkit-tap-highlight-color: rgba(0, 0, 0, 0);*/
        background-color: #333333!important;



    }

    .head-title {
        color: #dd4b39;

    }

    .title-group {
        .title {
            border-bottom: 1px solid #ebebeb;
            h3 {
                margin: 0 0px -3px;
            }
        }
        padding-bottom: 15px;

    }

    .RG {
        background: url(<%= asset_path 'activity_icons_4_915bbe063236330b.png' %>) no-repeat left top;
        background-position: -16px -128px;
        height: 32px;
        width: 32px;
        display: inline-block;
        margin: 0 0px 3px 0;
        vertical-align: middle;
    }


    #new_user{
        width: 360px;
        margin-right: 20px;
        float: left;
        border-right: 1px solid #ddd;
        padding-right: 20px;
    }

    .avatar {
        vertical-align: top;
        width: 25px;
        height: 25px;
    }

    .fb_login {
        padding-left: 30px;
        width: 500px;
        float: left;
    }

    .login_with.facebook {
        background: url(<%= asset_path 'facebook-button.png'%>) 0 0 no-repeat;
        color: white;
        display: block;
        font-size: 14px;
        font-weight: bold;
        height: 40px;
        line-height: 38px;
        /*margin: 0 0 5px;*/
        text-indent: 45px;
        text-shadow: #000 0 -1px 0;
        width: 200px;
        text-decoration: none;
        -webkit-text-stroke: 1px transparent;
    }

    .toolbar-back {
        vertical-align: bottom;
        padding-left: 10px;
        width: 50px;
    }

    .pull-bottom {
        /*vertical-align: bottom;*/
        padding-top: 10px;
    }

    .bare {
        margin-bottom: 20px;
    }

    .fee .indent label {
        float: none;
        font-weight: normal;
        font-size: 12px;
    }

    .pull-center {
        text-align: center;
    }

    .fb-like-box {
        padding-top: 20px;
    }


    /** sss **/


    .node {
        background: #fff;
        margin-bottom: 10px;
        border-bottom: #d1d1d1 1px solid;
        -moz-border-radius-bottomleft: 5px;
        -moz-border-radius-bottomright: 5px;
        -webkit-border-bottom-left-radius: 5px;
        -webkit-border-bottom-right-radius: 5px;
        border-bottom-left-radius: 5px;
        border-bottom-right-radius: 5px;
    }



    .node .content {
        margin-top: 5px;
        /*padding: 0 15px 0 15px;*/
        min-height: 120px;
    }

    .clear-block {
        display: block;
    }

    .sticky .content-title-box, .sticky .content-title-box a {
        background-color: #FED943;
        color: #993100!important;
    }

    .content-title-box {
        position: relative;
        /*padding: 5px 15px;*/
        font-weight: bold;
        background: #4c6e9c;
        margin: 0px;
        margin-bottom: 5px;
        -moz-border-radius-topleft: 5px;
        -moz-border-radius-topright: 5px;
        -webkit-border-top-left-radius: 5px;
        -webkit-border-top-right-radius: 5px;
        border-top-left-radius: 5px;
        border-top-right-radius: 5px;
        color: #FFFFFF;
    }

    .content-title-box a {
        color: #FFFFFF !important;
    }

    .sticky-badge {
        position: absolute;
        background: url(http://www.blognone.com/sites/all/themes/blognone41/images/border-featured.png) no-repeat;
        top: -3px;
        right: -3px;
        width: 64px;
        height: 64px;
    }

    .node .content {
        margin-top: -9px !important;
    }

    .sticky {
        background: #fff3d3 !important;
        border-color: #fdda42 !important;

    }

    .sticky .content {
        /*border-color: #fdda42 !important;*/
        border: #fdda42 1px solid;
        background: #fff3d3;
        box-shadow: none;
        padding: 0px ;
        padding-top: 5px;
    }

    .content-title-box h2 {
        font-size: 24px;
        margin: 0px;
        padding-left: 15px;
    }









